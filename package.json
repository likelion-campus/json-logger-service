{
  "name": "json-logger-service",
  "version": "1.0.20",
  "description": "Nest Json LoggerService implementation.",
  "main": "dist/index",
  "typings": "dist/index",
  "files": [
    "dist"
  ],
  "scripts": {
    "clean": "rimraf ./dist ./coverage",
    "tslint": "tslint -c tslint.json -t verbose 'spec/**/*.ts' 'src/**/*.ts'",
    "tslint:fix": "tslint --fix -c tslint.json -t verbose 'spec/**/*.ts' 'src/**/*.ts'",
    "ts": "tsc",
    "ts:watch": "tsc -w",
    "test": "jest --coverage",
    "build": "npm run clean; npm run ts",
    "prepublishOnly": "npm run build",
    "prets": "npm run clean; npm run tslint",
    "prets:watch": "npm run clean; npm run tslint:fix"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/marciopd/json-logger-service.git"
  },
  "keywords": [
    "json",
    "nest",
    "logger",
    "service"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@nestjs/common": "^6.0.0",
    "@nestjs/core": "^6.0.0",
    "@nestjs/platform-express": "^6.0.0",
    "bunyan": "^2.0.0",
    "on-finished": "^2.3.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^6.0.0"
  },
  "devDependencies": {
    "@types/jest": "^25.1.0",
    "@types/node": "^10.12.18",
    "@types/supertest": "^2.0.7",
    "jest": "^25.0.0",
    "rimraf": "^2.6.3",
    "ts-jest": "^25.0.0",
    "ts-mockito": "^2.5.0",
    "tslint": "^5.0.0",
    "tslint-config-airbnb": "^5.11.1",
    "tslint-microsoft-contrib": "^6.1.0",
    "typescript": "^3.4.3"
  }
}
